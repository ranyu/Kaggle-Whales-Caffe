# The train/test net protocol buffer definition
net: "train/classifier.prototxt"
test_state: { stage: 'test-on-train' }
test_iter: 600
test_interval: 1200
test_compute_loss: true
type: "AdaDelta"
gamma: 0.0001
base_lr: 0.1
momentum: 0.95
delta: 1e-6
weight_decay: 0.0005
power: 0.75
lr_policy: "fixed"
display: 600
max_iter: 45600
snapshot_prefix: "classifier"
solver_mode: GPU
